{
  "name": "permiakov/ood",
  "description": "OOD in examples",
  "authors": [
    {
      "name": "permiakov",
      "email": "dn.permyakov@gmail.com",
      "homepage": "https://github.com/permiakov"
    }
  ],
  "keywords": [
    "php",
    "ood"
  ],
  "require": {
    "myclabs/php-enum": "^1.7",
    "php": "^7.0",
    "squizlabs/php_codesniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Permiakov\\OOD\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Permiakov\\OOD\\Test\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "scripts": {
    "cs-check": "vendor/bin/phpcs  --standard=PSR12 src/ tests/",
    "cs-fix": "vendor/bin/phpcbf  --standard=PSR12 src/ tests/"
  }
}
